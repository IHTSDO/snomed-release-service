<beans xmlns="http://www.springframework.org/schema/beans"
		xmlns:context="http://www.springframework.org/schema/context"
		xmlns:mvc="http://www.springframework.org/schema/mvc"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="
		http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
		http://www.springframework.org/schema/context
		http://www.springframework.org/schema/context/spring-context-3.2.xsd
		http://www.springframework.org/schema/mvc
		http://www.springframework.org/schema/mvc/spring-mvc-3.2.xsd
		">

	<context:component-scan base-package="org.ihtsdo.buildcloud.controller" />

	<mvc:interceptors>
		<bean class="org.ihtsdo.buildcloud.security.SSOSecurityHandlerInterceptor"/>
	</mvc:interceptors>

	<import resource="servletContext.xml"/>
	<import resource="swaggerContext.xml"/>
	<import resource="jms-api-context.xml"/>
	<import resource="telemetry-jms-broker.xml"/>
	<import resource="classpath*:dataServiceContext.xml"/>

</beans>

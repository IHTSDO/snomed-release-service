<?xml version="1.0" encoding="UTF-8" ?>
<Configuration xmlns:xi="http://www.w3.org/2001/XInclude" packages="org.ihtsdo" status="info">
	<Appenders>
		<TelemetryEventAppender name="TelemetryEventAppender" env="${ENV_NAME}"/>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout pattern="%d{yyyyMMdd_HH:mm:ss} %-5p %C.%M - %m%n"/>
		</Console>
	</Appenders>
	<Loggers>
		<Root level="info">
			<AppenderRef ref="Console"/>
		</Root>
	</Loggers>
</Configuration>
